{
  "name": "eiffelsyntaxhighlighter",
  "displayName": "EiffelSyntaxHighlighter",
  "description": "Syntax highlighting for eiffel programming language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "eiffel",
        "aliases": [
          "Eiffel",
          "eiffel"
        ],
        "extensions": [
          ".e"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "eiffel",
        "scopeName": "source.e",
        "path": "./syntaxes/eiffel.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  }
}
